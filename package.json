{
  "name": "@prisma-cms/front-editor",
  "version": "2.17.1",
  "private": false,
  "main": "lib/App.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "yarn build-less && nwb build --copy-files",
    "build-app": "react-scripts build",
    "build-less": "node-less-chokidar src/styles/less",
    "start-ssr": "NODE_ENV=production node src/ssr",
    "start-ssr-dev": "NODE_ENV=production PRISMA_CMS_TIMELOG=false nodemon src/ssr",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "@prisma-cms/context": "1.3.2",
    "@prisma-cms/editor": "latest",
    "@prisma-cms/filters": "latest",
    "@prisma-cms/front": "latest",
    "@prisma-cms/query-builder": "latest",
    "@prisma-cms/uploader": "latest",
    "apollo-cms": "latest",
    "css": "^2.2.4",
    "graphql": "14.0.2",
    "moment": "^2.24.0",
    "path-to-regexp": "^3.0.0",
    "react-slick": "^0.24.0",
    "slick-carousel": "^1.8.1",
    "urijs": "^1.19.1",
    "vertical-timeline-component-for-react": "^1.0.6"
  },
  "devDependencies": {
    "@babel/core": "7.1.2",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@prisma-cms/performance": "latest",
    "cheerio": "^1.0.0-rc.3",
    "graphql-cli": "2.17.0",
    "node-less-chokidar": "^0.3.1",
    "nwb": "^0.23.0",
    "react-scripts": "2.1.2",
    "source-map-explorer": "^2.0.1"
  },
  "resolutions": {
    "@prisma-cms/context": "1.3.2",
    "graphql": "14.0.2",
    "react-router": "4.3.1",
    "react-router-dom": "4.3.1"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "description": "Front editor for @prisma-cms",
  "directories": {
    "lib": "lib"
  },
  "keywords": [
    "prisma-cms",
    "front",
    "components"
  ],
  "author": "Fi1osof",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/prisma-cms/front-editor.git"
  },
  "bugs": {
    "url": "https://github.com/prisma-cms/front-editor/issues"
  },
  "homepage": "https://github.com/prisma-cms/front-editor#readme"
}
